<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="stack" onMouseClicked="#mouse" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.navwestern.UserCreatePOIController">
   <children>
      <TextField fx:id="description" layoutX="1.0" layoutY="237.0" prefHeight="137.0" prefWidth="170.0" visible="false" />
      <ToolBar prefHeight="40.0" prefWidth="607.0">
        <items>
          <Button fx:id="homeButton" mnemonicParsing="false" onAction="#HomeButtonOnAction" prefHeight="25.0" prefWidth="60.0" text="Home" />
            <Button fx:id="createPOIButton" mnemonicParsing="false" onAction="#CreatePOIButtonOnAction" text="Create POI" />
        </items>
      </ToolBar>
      <Label layoutX="11.0" layoutY="164.0" prefHeight="17.0" prefWidth="80.0" text="Add New POI" visible="false" />
      <TextField fx:id="name" layoutX="11.0" layoutY="182.0" prefHeight="25.0" prefWidth="170.0" visible="false" />
      <Label fx:id="nameLabel" layoutX="11.0" layoutY="220.0" text="Description" visible="false" />
       <Label layoutX="15.0" layoutY="45.0" text="BUILDING" />
       <Label layoutX="147.0" layoutY="45.0" text="FLOOR" />
       <ChoiceBox fx:id="selectPOIBuildingBox" layoutX="15.0" layoutY="65.0" prefHeight="26.0" prefWidth="122.0" />
       <ChoiceBox fx:id="selectPOIFloorBox" layoutX="147.0" layoutY="65.0" prefHeight="26.0" prefWidth="92.0" />
       <ImageView fx:id="imageView" fitHeight="329.0" fitWidth="336.0" layoutX="250.0" layoutY="54.0" pickOnBounds="true">

       </ImageView>
       <Button fx:id="displayButton" layoutX="20.0" layoutY="90.0" mnemonicParsing="false" onAction="#DisplayMapButtonOnAction" text="Display" />
       <CheckBox fx:id="toggle" onAction="#onClick" />
       <Button fx:id="addPOI" layoutX="100.0" layoutY="170.0" mnemonicParsing="false" onAction="#addPOIOnClick" text="Add POI" />
   </children>
</AnchorPane>
